{
    "name": "Web4All",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Une extension axée sur l'accessibilité",
    "icons": {
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    
    "content_scripts": [
        {
            "js": [
                "Script/content.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}
    